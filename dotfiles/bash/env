# Set PATH, CDPATH, EDITOR, etc.

DOTFILES=$HOME/bin/dotfiles

PATH=/usr/local/bin:$PATH:/opt/local/bin:~/bin
DEV_DIR=/Users/travis/dev

export EMACS_CLIENT=emacsclient
export EMACS_SERVER=/Applications/Emacs.app/Contents/MacOS/Emacs
export EDITOR=eclient
export JAVA_HOME=/System/Library/Frameworks/JavaVM.framework/Versions/1.6/Home
